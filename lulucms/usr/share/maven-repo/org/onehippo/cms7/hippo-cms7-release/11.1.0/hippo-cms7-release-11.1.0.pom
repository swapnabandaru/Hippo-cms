      <artifactId>hippo-services</artifactId>
    </dependency>
    <dependency>
      <groupId>org.onehippo.cms7</groupId>
      <artifactId>hippo-repository-api</artifactId>
    </dependency>
    <dependency>
      <groupId>org.onehippo.cms7</groupId>
      <artifactId>hippo-repository-builtin</artifactId>
    </dependency>
  </dependencies>

  <build>
    <pluginManagement>
      <plugins>
        <!-- 'nullify' the inherited but 'Hippo only' specific maven-war-plugin and maven-remote-resources-plugin
              handling of LICENSE and NOTICE files as configured in the hippo-cms7-project parent.
        -->
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-war-plugin</artifactId>
          <version>${maven.plugin.war.version}</version>
          <configuration>
            <webResources combine.self="override">
            </webResources>
            <packagingExcludes combine.self="override">
            </packagingExcludes>
          </configuration>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-remote-resources-plugin</artifactId>
          <executions>
            <execution>
              <goals>
                <goal>process</goal>
              </goals>
              <configuration>
                <resourceBundles combine.self="override">
                </resourceBundles>
              </configuration>
            </execution>
          </executions>
        </plugin>
        <!-- end of 'nullify' inherited configuration -->
        <plugin>
          <groupId>org.onehippo.cms.l10n</groupId>
          <artifactId>hippo-cms-l10n-maven-plugin</artifactId>
          <version>${hippo.maven.plugin.l10n}</version>
          <extensions>true</extensions>
          <executions>
            <execution>
              <goals>
                <goal>include-locales</goal>
              </goals>
              <configuration>
                <artifactPrefixes>
                  <artifactPrefix>org.onehippo.cms7:hippo-addon-channel-manager,hippo-addon-channel-manager-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-plugin-content-blocks,hippo-plugin-content-blocks-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-plugin-content-tagging,hippo-plugin-content-tagging-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-plugin-dashboard-document-wizard,hippo-plugin-dashboard-document-wizard-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-plugin-gallerypicker,hippo-plugin-gallerypicker-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-cms-engine,hippo-cms-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-addon-hst-configuration-editor,hippo-addon-hst-configuration-editor-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-plugin-poll-addon-repository,hippo-plugin-poll-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-plugin-relateddocs,hippo-plugin-relateddocs-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-plugin-resourcebundle-editor,hippo-plugin-resourcebundle-editor-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-plugin-robotstxt,hippo-plugin-robotstxt-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-plugin-selections-addon-repository,hippo-plugin-selections-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-plugin-taxonomy-addon-repository,hippo-plugin-taxonomy-l10n</artifactPrefix>
                  <artifactPrefix>org.onehippo.cms7:hippo-essentials-components,hippo-essentials-components-l10n</artifactPrefix>
                </artifactPrefixes>
                <locales>${hippo.cms.locales}</locales>
              </configuration>
            </execution>
          </executions>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>

  <profiles>
    <profile>
      <id>cargo.run</id>
      <dependencies>
        <dependency>
          <groupId>org.onehippo.cms7</groupId>
          <artifactId>hippo-services-autoreload</artifactId>
          <scope>provided</scope>
